{
  "name": "mello-labs-angular-starter",
  "version": "2.2.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run format && ng serve",
    "format": "npm run format-glob -- \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format-glob": "prettier --write",
    "ng": "ng",
    "build": "ng build",
    "prod": "http-server ./dist -o -g",
    "build:prod": "ng build --target=production --environment=prod",
    "deploy": "ng build --prod --base-href /mello-labs-jam4pipeline/ && angular-cli-ghpages --no-silent",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "0.6.3",
    "@angular/animations": "6.0.3",
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
    "@angular/core": "6.0.3",
    "@angular/elements": "6.0.3",
    "@angular/forms": "6.0.3",
    "@angular/http": "6.0.3",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "@angular/router": "6.0.3",
    "@angular/service-worker": "6.0.3",
    "@mello-labs/api-tools": "4.0.0",
    "@mello-labs/datagrid": "2.0.0",
    "@mello-labs/form-tools": "2.0.0",
    "@mello-labs/ui": "1.2.0",
    "@ng-bootstrap/ng-bootstrap": "2.0.0",
    "@ngrx/store": "6.0.1",
    "angular-resizable-element": "3.0.0",
    "bootstrap": "4.0.0",
    "core-js": "2.4.1",
    "document-register-element": "1.7.2",
    "lodash": "4.17.4",
    "ngx-bootstrap": "^3.0.1",
    "rxjs": "6.2.0",
    "typescript-fsa": "2.5.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.3",
    "@angular/cli": "6.0.7",
    "@angular/compiler-cli": "6.0.3",
    "@angular/language-service": "6.0.3",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "2.0.2",
    "@types/lodash": "4.14.88",
    "@types/node": "6.0.60",
    "angular-cli-ghpages": "0.5.2",
    "codelyzer": "4.2.1",
    "font-awesome": "4.7.0",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "prettier": "1.11.1",
    "protractor": "5.1.2",
    "ts-node": "3.2.0",
    "tslint": "5.10.0",
    "tslint-angular": "1.1.2",
    "typescript": "2.7.2"
  }
}
